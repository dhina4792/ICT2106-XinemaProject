@model IEnumerable<XinemaProject.Models.Movies>

@{
    ViewBag.Title = "Movies";
   
}


<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-sm-6 col-md-6">
                <div class="thumbnail">

                    <div class="caption">
                        <h3>@Html.DisplayFor(modelItem => item.moviesName)</h3>
                      
                    </div>
                </div>
            </div>
        }
    </div>
</div>


