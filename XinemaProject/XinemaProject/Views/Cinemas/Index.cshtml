@model IEnumerable<XinemaProject.Models.Cinema>


@{
    ViewBag.Title = "Cinemas";
}




<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-sm-6 col-md-6">
                <div class="thumbnail">

                    <div class="caption">
                        <h3>@Html.DisplayFor(modelItem => item.CinemaName)</h3>

                    </div>
                </div>
            </div>
        }
    </div>
</div>